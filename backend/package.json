{
  "name": "profit_calculator_backend",
  "version": "0.0.1",
  "description": "",
  "author": "Ignacio Gonzalez Chemes",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "PROFIT_CALCULATOR_ENV=dev nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "PROFIT_CALCULATOR_ENV=prod node dist/main",
    "start:local": "PROFIT_CALCULATOR_ENV=local nest start src --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs -d ./ormconfig.ts",
    "typeorm:dev": "PROFIT_CALCULATOR_ENV=dev typeorm-ts-node-commonjs -d ./ormconfig.ts",
    "typeorm:local": "PROFIT_CALCULATOR_ENV=local typeorm-ts-node-commonjs -d ./ormconfig.ts",
    "migration:create": "node ./CreateMigration.js",
    "migration:run": "npm run typeorm:local migration:run",
    "migration:revert": "npm run typeorm:local migration:revert",
    "migration:show": "npm run typeorm:local migration:show"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.1",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^10.4.1",
    "@nestjs/typeorm": "^10.0.2",
    "@types/bcrypt": "^5.0.2",
    "@types/body-parser": "^1.19.5",
    "@types/uuid": "^10.0.0",
    "@types/webpack-env": "^1.18.5",
    "bcrypt": "^5.1.1",
    "better-console-log-plus": "^1.4.4",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.5",
    "helmet": "^7.1.0",
    "mariadb": "^3.3.1",
    "mysql2": "^3.11.0",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^10.1.4",
    "@nestjs/testing": "^10.4.1",
    "@types/express": "^4.17.21",
    "@types/jest": "29.5.12",
    "@types/node": "^22.5.2",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "29.7.0",
    "prettier": "^3.3.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.5.4",
    "webpack": "^5.94.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}